{
  "name": "chiti_by_antstuido",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "tsc:check": "tsc -p .",
    "patch": "patch-package",
    "ios": "react-native run-ios",
    "asset": "npx react-native-asset",
    "android": "react-native run-android",
    "pod": "cd ios && pod install && cd ..",
    "env-staging": "cp .env.staging .env",
    "env-production": "cp .env.production .env",
    "env-development": "cp .env.development .env",
    "eslint:check": "eslint . --ext .js,.ts,.tsx",
    "eslint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "start": "watchman watch-del-all && react-native start",
    "sha1": "cd android && ./gradlew signingReport && cd android",
    "clean": "yarn cache clean && cd android && ./gradlew clean && cd ..",
    "aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "CodePush:ios-demo": "appcenter codepush release-react -a ad.antstudio-gmail.com/Chia_tien_IOS -d Staging -t '>=1.0.0'",
    "CodePush:android-demo": "appcenter codepush release-react -a ad.antstudio-gmail.com/Chia_tien_Android -d Staging -t '>=1.0.0'",
    "splash": "react-native generate-bootsplash src/assets/images/Logo_Chiti.png --background-color=FFFFFF --logo-width=220 --assets-path=assets --flavor=main --platforms=android,ios"
  },
